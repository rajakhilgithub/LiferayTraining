<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="net.opentrends.candidate.model.Registration" />
	<class name="net.opentrends.candidate.model.impl.RegistrationImpl" table="candidate_Registration">
		<cache usage="read-write" />
		<id name="id" column="id_" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="city" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mobileNo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="photoFileEntryId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="resumeFileEntryId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>