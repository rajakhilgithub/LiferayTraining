<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="net.opentrends.training.service.model.Employee" />
	<class name="net.opentrends.training.service.model.impl.EmployeeImpl" table="emp_Employee">
		<cache usage="read-write" />
		<id name="employeeId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="employeeName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="houseName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="street" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="city" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="district" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="pincode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="department" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="designation" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="salary" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="adharno" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="fileEntryId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>